public class TemplateSetting implements MarkupExtension, PropertyChangeListener{
	private Class<Template> _template;
	private Class<TemplateSelector> _selector;
	private String _property;  
	private Node _container;
	private boolean _lightweight;
	private boolean _expandCollection;
	protected Map<Object, Template> itemsMap = new Map<Object, Template>();
	private Object _dataItem;
	
	public TemplateSetting(Object option){
		if(option["template"] != null){
			this._template = (Class<Template>) option["template"];
		}
		if(option["property"] != null){
			this._property = (String) option["property"];
		} 
	} 
	
	public Node container{
		&{
			return this._container;
		}
		+{
			this._container = value;
		}
	}
	
	public Object dataItem{
		&{
			return this._dataItem;
		}
		+{
			if(this._dataItem === value){
				return;
			}
			
			undoTemplate();
			
			if(this._dataItem != null){
				if(this._dataItem instanceof INotifyCollectionChanged){
					((INotifyCollectionChanged<Object>)this._dataItem).removeCollectionChangedListener(this.onCollectionChanged);
				}
			}

			if((this._dataItem = value) != null){
				if(this._dataItem instanceof INotifyCollectionChanged){
					((INotifyCollectionChanged<Object>)this._dataItem).addCollectionChangedListener(this.onCollectionChanged);
				}
				
				applyTemplate();
			}
		}
	}
	
	public void applyTemplate(){
		if(this._expandCollection && this._dataItem instanceof Collection){
			for(Object obj : (Collection<?>)this._dataItem){
				Template template = createTemplate(obj);
				Node[] node = template.applyTemplate(container, obj);
				itemsMap.set(obj, template);
			}
		} else {
			internalApplyTemplate(this._dataItem);
		}
	}
	
	private void internalApplyTemplate(Object data){
		Template template = createTemplate(data);
		Node[] node = template.applyTemplate(container, data);
		itemsMap.set(data, template);
	}
	
	/**
	 * 撤销模板展开的节点
	 */
	public void undoTemplate(){
		this.itemsMap.forEach((Template temp, Object data, Map<Object, Template> map)->{
			temp.undoTemplate(this.container);
		});
		itemsMap.clear();
	}
	
	public void invalidate(){
		if(container.dataContext.dataItem == null){
			this.dataItem = null;
		} else {
			this.dataItem = container.dataContext.dataItem[_property];
		}
	}
	
	protected CollectionChanged<Object> onCollectionChanged = (Object sender, CollectionChangedEvent<Object> event) ->{
		switch(event.action){
		case Add:
			Array<?> items = event.newItems;
			if(items == null || items.length <= 0){
				break;
			}
			for(Object item : items){
				Template template = createTemplate(item);
				Node[] root = template.applyTemplate(this._container, item);
				itemsMap.set(item, template);
			}
			break;
		case Reset:
			undoTemplate();
			break;
		case Remove:
			Template[] toBeRemoved = {};
			for(Object item : event.oldItems){
				Template temp = itemsMap.get(item);
				temp.undoTemplate(this._container);
				itemsMap.delete(item);
			}
			break;
		case Replace:
		case Move:
			
		}
	};
	
	public PropertyChange propertyChange = (Object source, PropertyChangeEvent event)->{
		this.dataItem = source[event.propertyName];
	};
	
	public Class<Template> template{
		&{
			return this._template;
		}
		+{
			this._template = value;
		}
	}
	
	public Class<TemplateSelector> selector{
		&{
			return this._selector;
		}
		+{
			this._selector = value;
		}
	}
	
	public String property{
		&{
			return this._property;
		}
		+{
			this._property = value;
		}
	}
	
	public boolean lightweight{
		&{
			return this._lightweight;
		}
		+{
			this._lightweight = value;
		}
	}
	
	private Template createTemplate(Object data){
		if(this._selector != null){
			TemplateSelector selector = (TemplateSelector) this._selector.newInstance(); 
			Template temp = selector.select(data);
			if(temp != null) return temp;
		}
		
		if(this.template != null) return (Template) this.template.newInstance();
		
		return null;
	}
	
	public native Object provideValue(Node target, String property, String targetProperty1) /*-{
		var r = new (this._itemControlClazz.factory)(target, this._path, this._template);
		return r;
	}-*/;

	@Override
	public void propertyChange(java.lang.Object source, java.lang.PropertyChangeEvent evt) {
		// TODO Auto-generated method stub
		
	}
}

