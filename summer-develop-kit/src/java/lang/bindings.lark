public abstract class Template {
	protected Node[] _rootNodes = {};
	protected BeforeCallback _before;
	protected AfterCallback _after;
	protected boolean _expandCollection;
	protected Object _data;
	
	public boolean expandCollection{
		&{
			return this._expandCollection;
		}
		+{
			this._expandCollection = value;
		}
	};

	public BeforeCallback before{
		&{
			return this._before;
		}
		+{
			this._before = value;
		}
	}
	
	public AfterCallback after{
		&{
			return this._after;
		}
		+{
			this._after = value;
		}
	};
	
	public Node[] rootNodes{
		&{
			return this._rootNodes;
		}
	}
	
	protected Object data{
		&{
			return this._data;
		}
		+{
			this._data = value;
		}
	}
	
	public final Node[] applyTemplate(Node parent, Object data){
		return _rootNodes = doApplyTemplate(parent, data);
	}
	
	protected Node[] doApplyTemplate(Node parent, Object data){
		throw new Error(0, "illegal call!");
	}

	public void undoTemplate(Node parent) {
		_rootNodes.forEach((Node node, int index, Array<Node> array)->{
			parent.removeChild(node);
		});
	}
	
}

class SomeTemplate extends Template{
	protected Node[] doApplyTemplate(Node parent, Object data){
		Node[] result = {};
		Node root;
		//第一个根节点
		result.push(root = ()->{
			if(this._before != null){
				this._before(data);
			}
			__n = __c("div");
			if(this._expandCollection){
				__n.dataContext = new DataContext(data);
			}
			
			if(this._after != null){
				this._after(data, result);
			}
			
			//建立孩子节点
			
		}(parent));
		//第二个根节点
		result.push(root = ()->{
			if(this._before != null){
				this._before(data);
			}
			__n = __c("div");
			if(this._expandCollection){
				__n.dataContext = new DataContext(data);
			}
			
			if(this._after != null){
				this._after(data, result);
			}
			
			//建立孩子节点
			
		}(parent));
//		...其他孩子节点
		
		return result;
	}

}

